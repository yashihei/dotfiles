" Font
"set guifont=Ricty\ 13
"set guifont=Consolas:h12
"set guifontwide=MS_Gothic:h11

" ColorScheme
syntax enable
colorscheme desert

" Etc
" すけすけ
"gui
"set transparency=220
" ツールバーを削除
set guioptions-=T
set guioptions-=r
set guioptions-=l
set guioptions-=L

" ビープ音
set vb t_vb=
" コマンドラインの高さ
set cmdheight=2
