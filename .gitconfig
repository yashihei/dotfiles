[user]
    name = yashihei
    email = yashihei@gmail.com
[color]
    ui = auto
[alias]
    graph = log --graph --oneline --decorate
    st = status -sb
    cm = commit
    df = diff
    dfc = diff --cached
    co = checkout
    cb = checkout -b
[core]
    editor = vim
    filemode = false
    pager = LESS=-R less
    autocrlf = true
    quotepath = false
[push]
    default = upstream
